@model IEnumerable<KS.Core.Model.PHONG>

@{
    ViewBag.Title = "Home Page";
}

<h2>Tổng cộng có @Model.Count() phòng:</h2>
@foreach(var phong in Model)
{
    string style = (phong.Deleted == true ? "style='color:red;'" : "");
    <p @Html.Raw(style)>@phong.MaPhong |    @phong.SDTPhong</p>
    
}